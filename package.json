{
	"name": "markdown-to",
	"version": "1.1.7",
	"description": "一个将markdown文件转换成vue、html、jsx、tsx格式文件的工具，支持批量转换与翻译。",
	"main": "build/index.js",
	"homepage": "https://github.com/const-love-365-10000/markdown-to",
	"bugs": {
		"url": "https://github.com/const-love-365-10000/markdown-to/issues"
	},
	"scripts": {
		"start": "tsc && node build/start.js",
		"start:tocFile": "tsc && node build/start.js --toc-file",
		"start:translate": "tsc && node build/start.js --translate",
		"test": "node test/test.mjs",
		"publish": "tsc && npm publish"
	},
	"keywords": [
		"markdown",
		"markdown-to"
	],
	"module": "build/index.js",
	"types": "type/global.d.ts",
	"author": "yujin321",
	"license": "MIT",
	"dependencies": {
		"@vitalets/google-translate-api": "^8.0.0",
		"commander": "^9.2.0",
		"highlight.js": "^11.5.1",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.4",
		"markdown-it-toc-done-right": "^4.2.0"
	},
	"devDependencies": {
		"@types/markdown-it": "^12.2.3",
		"@types/node": "^17.0.35",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"eslint": "^8.16.0",
		"typescript": "^4.6.4"
	}
}
